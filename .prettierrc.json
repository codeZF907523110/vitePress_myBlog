{
  "printWidth": 120,
  "semi": false,
  "vueIndentScriptAndStyle": true,
  "singleQuote": true,
  "trailingComma": "none",
  "arrowParens": "avoid",
  "proseWrap": "never",
  "htmlWhitespaceSensitivity": "ignore",
  "endOfLine": "auto",
  "overrides": [
    {
      "files": "*.md",
      "options": {
        "proseWrap": "preserve",
        "tabWidth": 2
      }
    }
  ]
}
